{
  "name": "Swarm Syntax Highlighting",
  "description": "Syntax Highlighting, in Swarm",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
        "matches": ["http://swarm/*"],
        "js": ["inject.js"],
        "css": ["main.css", "highlight.min.css"]
    }
  ],
    "web_accessible_resources": [
        "highlight.min.js",
        "escape-html.js",
        "main.js"
    ]
}
