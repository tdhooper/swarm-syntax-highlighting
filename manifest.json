{
  "name": "Swarm Syntax Highlighting",
  "description": "Syntax Highlighting, in Swarm",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
        "matches": ["http://swarm/*"],
        "js": ["inject.js"],
        "css": ["prism.css"]
    }
  ],
    "web_accessible_resources": [
        "prism.js",
        "escape-html.js",
        "main.js"
    ]
}
